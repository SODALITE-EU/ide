<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
			contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
			default="true"
			extensions="aadm"
			id="org.sodalite.dsl.AADM"
			name="AADM Editor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="org.sodalite.dsl.AADM.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="org.sodalite.dsl.AADM.validate">
		<activeWhen>
			<reference
					definitionId="org.sodalite.dsl.AADM.Editor.opened">
			</reference>
		</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="org.sodalite.dsl.AADM.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="org.sodalite.dsl.AADM.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="org.sodalite.dsl.AADM.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="org.sodalite.dsl.AADM"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
		<definition id="org.sodalite.dsl.AADM.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="org.sodalite.dsl.AADM"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="org.sodalite.dsl.AADM"
			name="AADM">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
		</page>
		<page
			category="org.sodalite.dsl.AADM"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="org.sodalite.dsl.AADM.coloring"
			name="Syntax Coloring">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
		</page>
		<page
			category="org.sodalite.dsl.AADM"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="org.sodalite.dsl.AADM.templates"
			name="Templates">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="org.sodalite.dsl.AADM"
			name="AADM">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="org.sodalite.dsl.ui.keyword_AADM"
			label="AADM"/>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
	<command
			description="Trigger expensive validation"
			id="org.sodalite.dsl.AADM.validate"
			name="Validate">
	</command>
	<!-- copy qualified name -->
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			 <command
				 commandId="org.sodalite.dsl.AADM.validate"
				 style="push"
				 tooltip="Trigger expensive validation">
			<visibleWhen checkEnabled="false">
				<reference
					definitionId="org.sodalite.dsl.AADM.Editor.opened">
				</reference>
			</visibleWhen>
		</command>
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="org.sodalite.dsl.AADM.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="menu:edit?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="org.sodalite.dsl.AADM.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="org.sodalite.dsl.AADM.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
					<reference definitionId="org.sodalite.dsl.AADM.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
			commandId="org.eclipse.xtext.ui.editor.FindReferences">
			<activeWhen>
				<reference
					definitionId="org.sodalite.dsl.AADM.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type
			base-type="org.eclipse.core.runtime.text"
			file-extensions="aadm"
			id="org.sodalite.dsl.AADM.contenttype"
			name="AADM File"
			priority="normal">
		</content-type>
	</extension>
	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="aadm">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="aadm">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for org.sodalite.dsl.AADM -->
	<extension
			id="aadm.check.fast"
			name="AADM Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="aadm.check.normal"
			name="AADM Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="aadm.check.expensive"
			name="AADM Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="org.sodalite.dsl.AADM"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.validation.ValidatorPreferencePage"
			id="org.sodalite.dsl.AADM.validator.preferencePage"
			name="Errors/Warnings">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
		</page>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="aadm"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="org.sodalite.dsl.AADM"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="org.sodalite.dsl.AADM.compiler.preferencePage"
			name="Compiler">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.propertyPages">
		<page
			category="org.sodalite.dsl.AADM"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="org.sodalite.dsl.AADM.compiler.propertyPage"
			name="Compiler">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="org.sodalite.dsl.AADM.OpenGeneratedCode"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference definitionId="org.sodalite.dsl.AADM.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
			<activeWhen>
				<reference definitionId="org.sodalite.dsl.AADM.Editor.opened" />
			</activeWhen>
		</handler>
	</extension>
	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler 
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="org.sodalite.dsl.AADM.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
  <handler
        class="org.sodalite.dsl.ui.handlers.AADMSaveHandler"
        commandId="org.sodalite.dsl.AADM.ui.handler.saveAADM">
  </handler>
  <handler
        class="org.sodalite.dsl.ui.handlers.AADMDeployHandler"
        commandId="org.sodalite.dsl.AADM.ui.handler.deployAADM">
  </handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
        description="Open the quick outline."
        id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
        name="Quick Outline">
		</command>
  <category
        id="org.sodalite.dsl.AADM.ui.category"
        name="Sodalite">
  </category>
  <command
        categoryId="org.sodalite.dsl.AADM.ui.category"
        defaultHandler="org.sodalite.dsl.ui.handlers.AADMSaveHandler"
        description="Save the AADM into the KB"
        id="org.sodalite.dsl.AADM.ui.command.saveAADM"
        name="saveAADM">
  </command>
  <command
        categoryId="org.sodalite.dsl.AADM.ui.category"
        defaultHandler="org.sodalite.dsl.ui.handlers.AADMDeployHandler"
        description="Deploy the AADM using the Sodalite IoC layer"
        id="org.sodalite.dsl.AADM.ui.command.deployAADM"
        name="deployAADM">
  </command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="org.sodalite.dsl.AADM.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
  <menuContribution
        allPopups="false"
        locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
     <menu
           label="Sodalite">
        <command
              commandId="org.sodalite.dsl.AADM.ui.command.saveAADM"
              id="org.sodalite.dsl.AADM.ui.popup.command.saveAADM"
              label="Save AADM  in the KB"
              style="push"
              tooltip="Save AADM into the remote Sodalite Knowledge Base">
           <visibleWhen
                 checkEnabled="false">
              <with
                    variable="activeMenuSelection">
                 <iterate
                       ifEmpty="false">
                    <adapt
                          type="org.eclipse.core.resources.IResource">
                       <test
                             property="org.eclipse.core.resources.name"
                             value="*.aadm">
                       </test>
                    </adapt>
                 </iterate>
              </with>
           </visibleWhen>
        </command>
        <command
              commandId="org.sodalite.dsl.AADM.ui.command.deployAADM"
              label="Deploy AADM"
              style="push"
              tooltip="Deploy the AADM  using the Sodalite IoC layer">
           <visibleWhen
                 checkEnabled="false">
              <with
                    variable="activeMenuSelection">
                 <iterate
                       ifEmpty="false">
                    <adapt
                          type="org.eclipse.core.resources.IResource">
                       <test
                             property="org.eclipse.core.resources.name"
                             value="*.aadm">
                       </test>
                    </adapt>
                 </iterate>
              </with>
           </visibleWhen>
        </command>
     </menu>
  </menuContribution>
	</extension>
	<!-- quickfix marker resolution generator for org.sodalite.dsl.AADM -->
	<extension
			point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="org.sodalite.dsl.AADM.ui.aadm.check.fast">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="org.sodalite.dsl.AADM.ui.aadm.check.normal">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="org.sodalite.dsl.AADM.ui.aadm.check.expensive">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
	</extension>
	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler 
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="org.sodalite.dsl.AADM.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="org.sodalite.dsl.AADM.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="org.sodalite.dsl.AADM"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
			id="org.sodalite.dsl.AADM.refactoring"
			name="Refactoring">
			<keywordReference id="org.sodalite.dsl.ui.keyword_AADM"/>
		</page>
	</extension>
	<extension point="org.eclipse.compare.contentViewers">
		<viewer id="org.sodalite.dsl.AADM.compare.contentViewers"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="aadm">
		</viewer>
		<contentTypeBinding
			contentTypeId="org.sodalite.dsl.AADM.contenttype"
			contentViewerId="org.sodalite.dsl.AADM.compare.contentViewers" />
	</extension>
	<extension point="org.eclipse.compare.contentMergeViewers">
		<viewer id="org.sodalite.dsl.AADM.compare.contentMergeViewers"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="aadm" label="AADM Compare">
		</viewer>
		<contentTypeBinding
			contentTypeId="org.sodalite.dsl.AADM.contenttype"
			contentMergeViewerId="org.sodalite.dsl.AADM.compare.contentMergeViewers" />
	</extension>
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider id="org.sodalite.dsl.AADM.editors.documentProviders"
			class="org.sodalite.dsl.ui.AADMExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
			extensions="aadm">
		</provider>
	</extension>
	<extension point="org.eclipse.team.core.fileTypes">
		<fileTypes
			extension="aadm"
			type="text">
		</fileTypes>
	</extension>
</plugin>
